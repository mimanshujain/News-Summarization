<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <% include partials/template/defaultjs.ejs %>
    <link type="text/css" href="/stylesheets/ui-lightness/jquery-ui-1.8.21.custom.css" rel="stylesheet" />
    <link type="text/css" href="/stylesheets/lda.css" rel="stylesheet" />
    <link type="text/css" href="/stylesheets/style.css" rel="stylesheet" />
	<link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.5.0/pure-min.css">

	<script type="text/javascript" src="/javascripts/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
    <script type="text/javascript" src="/javascripts/jquery-ui-1.8.21.custom.min.js"></script>
    <script src="/javascripts/jquery.tagcanvas.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/javascripts/stopwords.js"></script>
    <script type="text/javascript" src="/javascripts/lda.js"></script>
    <script type="text/javascript" src="http://cdn.knightlab.com/libs/timeline/latest/js/storyjs-embed.js"></script>
    <!--For topic handling-->
    <script type="text/javascript" src="/javascripts/topicwise.js"></script>
    <!--For Crawling the API's-->
    <script type="text/javascript" src="/javascripts/Crawler.js"></script>

    <% include partials/template/head.ejs %>
</head>
<body>

    <section>
        <div id="search_back" class="center_nm">
            <div id="search_div" class="center">
                <input id="search_field" type="text" style="height:30px; width:300px; top:6px; position:relative;" class="css-input"></input>
                <button type="submit" style="height:30px; top:6px; position:relative;" onclick=crawlGuardian() 
					class="pure-button pure-button-primary">Submit</button>
            </div>
        </div>
    </section>
	<section>
		<div id="topicaldivback" class="center_nm rotate180">
			<div id="topicaldiv" class="center">
				<div id="topiclist" class="rotate180">
					<input id="btn_topic[1]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[2]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[3]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[4]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[5]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[6]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[7]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[8]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[9]" type="button" style="height:30px; top:-9px; position:relative;"></input>
					<input id="btn_topic[10]" type="button" style="height:30px; top:-9px; position:relative;"></input>
				</div>
			</div>
		</div>
	</section
    <section>
        <div id="my-timeline"></div>
    </section>
    <input type="hidden" id="hdnMessage" value="0">



    <section>
        <label id='lblMessage'>Welcome to Kuch bhi..Do whatever you want to do</label>
        <div class="lower_border">
            <div id="topiccloud"></div>
            <div id="results"></div>
        </div>
    </section>
</body>
</html>